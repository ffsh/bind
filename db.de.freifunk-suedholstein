$TTL    900
$ORIGIN freifunk-suedholstein.de.
@       IN      SOA     ns3.nsentry.de. info.freifunk-suedholstein.de. (
                     2018033001         ; Serial
                          28800         ; Refresh
                           7200         ; Retry
                         604800         ; Expire
                            900 )       ; Negative Cache TTL

@		IN		NS	ns3.nsentry.de.
		IN		NS	ns4.nsentry.de.
		IN      MX      10 tux.tuks.in.
		IN      A       176.9.202.74
www     IN      A       176.9.202.74

